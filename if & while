// Nicolas Lopez.cpp : Defines the entry point for the console application.
//

#include "stdafx.h"
#include <iostream>
#include "conio.h"


using namespace std;
int main()
{int i,N;
cout <<"ingrese N\n";
cin>>N;

while(i<=N)
(cout<<i<<"\n";
i=i+1;)
	getch();
}

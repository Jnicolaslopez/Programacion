Naturales
// Nicolas Lopez.cpp : Defines the entry point for the console application.
//

#include "stdafx.h"
#include <iostream>
#include "conio.h"


using namespace std;
int main()
{int i,N;
cout <<"ingrese N\n";
cin >>N;

for (i=1;i<=N;i++)
cout<<i<<"\n";
	getch();
}






Impares
// Nicolas Lopez.cpp : Defines the entry point for the console application.
//

#include "stdafx.h"
#include <iostream>
#include "conio.h"


using namespace std;
int main()
{int i,N;
cout <<"ingrese N\n";
cin >>N;
i=1;

while (i<=N)
{cout<<i;
	i++
	getch();
}







Pares
// Nicolas Lopez.cpp : Defines the entry point for the console application.
//

#include "stdafx.h"
#include <iostream>
#include "conio.h"


using namespace std;
int main()
{int i,N;
cout <<"ingrese N\n";
cin >>N;

for (i=2;i<=N;i=i+2)
cout<<i<<"\n";
	getch();
}

